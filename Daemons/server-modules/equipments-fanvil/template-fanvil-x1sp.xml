<?xml version="1.0" encoding="UTF-8"?>
<sysConf>
  <Version>2.0000000000</Version>
  <mm>
    <SelectYourTone>{{{dialingtone}}}</SelectYourTone>
    <capability>
      <AudioCodecSets>{{{codecconfig}}}</AudioCodecSets>
    </capability>
  </mm>
  <sip>
    <line index="1">
      <PhoneNumber>{{{username}}}</PhoneNumber>
      <DisplayName>{{{displayname}}}</DisplayName>
      <SipName>{{{username}}}</SipName>
      <RegisterAddr>{{{server1addr}}}</RegisterAddr>
      <RegisterPort>{{{server1port}}}</RegisterPort>
      <RegisterUser>{{{username}}}</RegisterUser>
      <RegisterPswd>{{{password}}}</RegisterPswd>
      <LocalDomain>{{{domain}}}</LocalDomain>
      <VoiceCodecMap>{{{codecconfig}}}</VoiceCodecMap>
      <EnableReg>1</EnableReg>
      <EnableDND>0</EnableDND>
    </line>
  </sip>
  <call>
    <port index="1">
      <EnableDND>0</EnableDND>
      <EnableIntercom>0</EnableIntercom>
      <AllowIPCall>0</AllowIPCall>
    </port>
    <basic>
      <DialbyPound>0</DialbyPound>
      <DialFixedLength>0</DialFixedLength>
      <DialbyTimeout>0</DialbyTimeout>
      <EnableEOneSixFour>0</EnableEOneSixFour>
    </basic>
{{{dialplan}}}    <alertInfo index="1">
      <Text>internal</Text>
      <Line>-1</Line>
      <RingType>1</RingType>
    </alertInfo>
    <alertInfo index="2">
      <Text>external</Text>
      <Line>-1</Line>
      <RingType>2</RingType>
    </alertInfo>
  </call>
  <phone>
    <MenuPassword>******</MenuPassword>
    <EnableKeyLock>0</EnableKeyLock>
    <EmergencyCall>{{{emergencynumber}}}</EmergencyCall>
    <LineDisplayFormat>$name</LineDisplayFormat>
    <EnableMWITone>0</EnableMWITone>
    <display>
      <LCDTitle>{{{displayname}}}</LCDTitle>
      <DefaultLanguage>{{{language}}}</DefaultLanguage>
    </display>
    <date>
      <EnableSNTP>1</EnableSNTP>
      <SNTPServer>{{{ntpserver1}}}</SNTPServer>
      <SecondSNTPServer>{{{ntpserver2}}}</SecondSNTPServer>
      <TimeZone>{{{timezone}}}</TimeZone>
      <TimeZoneName>{{{timezonename}}}</TimeZoneName>
    </date>
    <timeDisplay>
      <EnableTimeDisplay>1</EnableTimeDisplay>
      <TimeDisplayStyle>{{{timeformat}}}</TimeDisplayStyle>
      <DateDisplayStyle>{{{dateformat}}}</DateDisplayStyle>
      <DateSeparator>{{{dateseparator}}}</DateSeparator>
    </timeDisplay>
    <softkey>
      <SoftkeyMode>0</SoftkeyMode>
      <SoftKeyExitStyle>2</SoftKeyExitStyle>
      <DesktopSoftkey>history;dss1;;menu;</DesktopSoftkey>
      <TalkingSoftkey>hold;xfer;conf;end;</TalkingSoftkey>
      <RingingSoftkey>accept;none;forward;reject;</RingingSoftkey>
      <AlertingSoftkey>end;none;none;none;</AlertingSoftkey>
      <XAlertingSoftkey>end;none;none;xfer;</XAlertingSoftkey>
      <ConferenceSoftkey>hold;none;split;end;</ConferenceSoftkey>
      <WaitingSoftkey>xfer;accept;reject;end;</WaitingSoftkey>
      <EndingSoftkey>repeat;none;none;end;</EndingSoftkey>
      <DialerPreSoftkey>send;2aB;delete;exit;</DialerPreSoftkey>
      <DialerCallSoftkey>send;2aB;delete;exit;</DialerCallSoftkey>
      <DialerXferSoftkey>delete;xfer;send;exit;</DialerXferSoftkey>
      <DialerCfwdSoftkey>send;2aB;delete;exit;</DialerCfwdSoftkey>
      <DesktopClick>history;status;none;none;none;</DesktopClick>
      <DailerClick>pline;nline;none;none;none;</DailerClick>
      <RingingClick>none;none;none;none;none;</RingingClick>
      <CallClick>none;none;none;none;none;</CallClick>
      <DesktopLongPress>status;none;none;none;reset;</DesktopLongPress>
      <DialerConfSoftkey>contact;clogs;redial;video;cancel;</DialerConfSoftkey>
    </softkey>
  </phone>
  <dsskey>
    <DSSLongPressAction>0</DSSLongPressAction>
    <internal index="1">
{{{linekey}}}    </internal>
  </dsskey>
  <web>
    <account index="1">
      <Name>{{{webadminname}}}</Name>
      <Password>{{{webadminpassword}}}</Password>
      <Level>10</Level>
    </account>
    <account index="2">
      <Name>{{{webusername}}}</Name>
      <Password>{{{webuserpassword}}}</Password>
      <Level>5</Level>
    </account>
  </web>
  <fwCheck>
    <EnableAutoUpgrade>1</EnableAutoUpgrade>
    <UpgradeServer1>tftp://{{{tftpserver}}}/Fanvil/X1SP</UpgradeServer1>
  </fwCheck>
  <record>
    <Enabled>0</Enabled>
  </record>
</sysConf>
