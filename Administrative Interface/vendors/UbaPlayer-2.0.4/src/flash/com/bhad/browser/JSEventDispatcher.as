package com.bhad.browser { import flash.external.*;import com.asorg.browser.*; public dynamic class JSEventDispatcher{   public static function dispatchEvent(event:String):void{     var script_js:XML = <script>      <![CDATA[        function(id,event){           /*******************		   IE (8 and older) doesn't support custom events which prevents us from 		   using pure JS to trigger the DOM events from Flash as such:		   		   var flashObject = document.getElementById(id);           var evObj = document.createEvent('Events');           evObj.initEvent(event, true, true);           window.dispatchEvent(evObj);		   		   I took the easy way out by using jQuery trigger. This function should 		   test to see if jQuery is included in the page. I'll add that later.		   		   ********************/		   		   $(window).trigger(event);        }      ]]>   </script>    ExternalInterface.call(script_js, EmbedObject.getId(), event);   }  }}